{
  "manifest_version": 2,
  "name": "Paramel",
  "description": "Remove unnecessary URL parameters for bookmarks.",
  "version": "1.0.0",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "48": "images/icon48.png"
    },
    "default_title": "Paramel"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["tabs"]
}
